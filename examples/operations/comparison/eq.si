import { output };

// Boolean
output(true == true);  // true
output(true == false); // false

// Int
output(1 == 1);        // true
output(1 == 2);        // false

// Float
output(1.23 == 1.23);  // true
output(1.23 == 2.34);  // false

// Mixed
output(1 == 1.0);      // true
output(1 == 1.23);     // false
