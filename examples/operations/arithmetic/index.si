import { output };

// Integer
output(1 + 2);  // 3
output(1 - 2);  // -1
output(1 * 2);  // 2
output(7 % 3);  // 1

// Float
output(1.23 + 2.34);  // 3.570000
output(1.23 - 2.34);  // -1.110000
output(1.23 * 2.34);  // 2.878200
output(7.5 % 3.5);    // 0.500000
output(1.23 / 2.34);  // 0.525641
output(1.23 / 0.0);   //

// Mixed
output(1 + 1.23);      // 2.230000
output(1 - 1.23);      // -0.230000
output(1 * 1.23);      // 1.230000
output(7 % 3.0);       // 1.000000
